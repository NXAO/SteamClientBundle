{
    "name": "nxao/steam-client-bundle",
    "type": "library",
    "description": "Nxao SteamClient component",
    "homepage": "https://github.com/NXAO/SteamClientBundle",
    "license": "MIT",
    "keywords" : [
        "steam","steam-api","symfony-bundle","symfony5","annotations","php7","php-library","api-client","steam-client"
    ],
    "authors": [
        {
            "name": "Oleg Charnyshevich",
            "email": "admin@love4dev.ru"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/framework-bundle": "^5.0"
    },
    "require-dev": {
        "symfony/console": "^5.0",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/var-dumper": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "autoload": {
        "psr-4": { "Nxao\\Component\\SteamClient\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
